<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.Example.iJam" >

    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/picture1"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.Example.iJam.MainActivity"
            android:label="@string/title_activity_main" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.Example.iJam.SecondActivity"
            android:label="@string/title_activity_second" />
        <activity
            android:name="com.Example.iJam.ThirdActivity"
            android:label="@string/title_activity_third" />
        <activity
            android:name="com.Example.iJam.FirstFragment"
            android:label="@string/title_activity_first_fragment" >
        </activity>
        <activity
            android:name="com.Example.iJam.SecondFragment"
            android:label="@string/title_activity_second_fragment" >
        </activity>
        <activity
            android:name="com.Example.iJam.ThirdFragment"
            android:label="@string/title_activity_third_fragment" >
        </activity>
        <activity
            android:name=".signup"
            android:label="@string/title_activity_signup" >
        </activity>
        <activity
            android:name="com.Example.iJam.signin"
            android:label="@string/title_activity_signin" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.Example.iJam.testsignup"
            android:label="@string/title_activity_testsignup" >
        </activity>
    </application>

</manifest>
